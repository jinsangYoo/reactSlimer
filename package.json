{
  "name": "reactslimer",
  "version": "2.9.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint --ext .tsx --ext .ts src/",
    "format": "prettier --check ./src",
    "start": "rm -rf reactSlimer/dist && tsc -w --outDir reactSlimer/dist",
    "start:expo": "rm -rf reactSlimerWithExpo/dist && tsc -w --outDir reactSlimerWithExpo/dist",
    "prebuild": "node -p \"'export const LIB_VERSION = ' + JSON.stringify(require('./package.json').version)\" > src/version.ts",
    "prepare": "npm run prebuild && rm -rf dist && tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "bugs": {
    "email": "milkybboy@gmail.com"
  },
  "author": "jinsang yoo",
  "license": "MIT",
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "@react-native-async-storage/async-storage": "1.x",
    "@react-native-community/netinfo": "9.x",
    "react-native-device-info": ">=8.4.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jinsangYoo/reactSlimer"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/babel__core": "^7.20.0",
    "@types/react": "^18.0.28",
    "@types/react-native": "^0.71.3",
    "eslint": "^8.34.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "lodash": "^4.17.21",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-native": "^0.71.3",
    "typescript": "^4.9.5"
  },
  "resolutions": {
    "react-native-device-info": "^8.4.9"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-community/netinfo": "^9.3.7",
    "axios": "^1.3.3",
    "events": "^3.3.0",
    "react-native-device-info": "^8.4.9"
  }
}
